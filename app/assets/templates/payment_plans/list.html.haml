.row
  .col-xs-12.col-lg-3.list
    %h2 Payment Plans
    %br
      %div(ng-repeat="paymentPlan in paymentPlans")
        %a(ui-sref=".detail({id:paymentPlan.id})" ng-bind="paymentPlan.name")
