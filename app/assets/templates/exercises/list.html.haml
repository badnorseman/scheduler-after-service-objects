.row
  .col-xs-12.col-lg-3.list
    %h2 Exercises
    %br
      %div(ng-repeat="exercise in exercises")
        %a(ui-sref=".detail({id:exercise.id})" ng-bind="exercise.name")
    %br
    %button.new(ui-sref=".detail") New
    %span
    %button.back(ui-sref="dashboard") Close
  .col-xs-12.col-lg-9.detail
    %div(ui-view)
